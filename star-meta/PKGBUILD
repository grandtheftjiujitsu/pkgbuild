pkgname=star-meta
pkgver=0.1
pkgrel=1.1
pkgdesc="Metapackage to install Perl6 modules included in Rakudo Star"
arch=('any')
depends=('zef')
groups=('perl6')
url="https://github.com/rakudo/star/blob/master/modules/MODULES.txt"
license=('None')
install=$pkgname.install
